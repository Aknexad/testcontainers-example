{
  "name": "integration-testing-docker",
  "version": "1.0.0",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test-local": "glob -c \"tsx --env-file .env --env-file .env.test --test --test-reporter spec \" \"./test/**/*.test.ts\"",
    "test": "DEBUG=testcontainers*  npm run test-local",
    "start": "npm run build && node dist/run.js",
    "dev": "ts-node-dev --poll --env-file=.env  src/server.ts",
    "build": "rimraf ./dist && npx tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/supertest": "^6.0.2",
    "express": "^4.21.1",
    "mongoose": "^8.7.2"
  },
  "devDependencies": {
    "@testcontainers/mongodb": "^10.13.2",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.8",
    "glob": "^11.0.0",
    "supertest": "^7.0.0",
    "testcontainers": "^10.13.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
